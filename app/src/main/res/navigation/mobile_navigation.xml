<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_employee">

    <fragment
        android:id="@+id/navigation_employee"
        android:name="me.kzaman.firebasecrud.ui.employee_list.EmployeeFragment"
        android:label="Employee List"
        tools:layout="@layout/fragment_employee_list" >
    </fragment>

    <fragment
        android:id="@+id/navigation_add_employee"
        android:name="me.kzaman.firebasecrud.ui.new_employee.NewEmployeeFragment"
        android:label="Add Employee"
        tools:layout="@layout/fragment_new_employee" >
    </fragment>

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="me.kzaman.firebasecrud.ui.dashboard.DashboardFragment"
        android:label="Dashboard"
        tools:layout="@layout/fragment_dashboard" >
    </fragment>

    <fragment
        android:id="@+id/navigation_notification"
        android:name="me.kzaman.firebasecrud.ui.notification.NotificationFragment"
        android:label="Notifications"
        tools:layout="@layout/fragment_notification" />
</navigation>